{
  "version": 1,
  "projects": {
    "my-lib": {
      "projectType": "library",
      "root": "packages/my-lib",
      "sourceRoot": "packages/my-lib/src",
      "prefix": "test-secondary",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/packages/my-lib"],
          "options": {
            "project": "packages/my-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/my-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/my-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/my-lib"],
          "options": {
            "jestConfig": "packages/my-lib/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/my-lib/src/**/*.ts",
              "packages/my-lib/src/**/*.html"
            ]
          }
        }
      }
    },
    "my-lib-secondary-one": {
      "projectType": "library",
      "root": "packages/my-lib/secondary-one",
      "sourceRoot": "packages/my-lib/secondary-one/src",
      "prefix": "test-secondary",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/packages/my-lib/secondary-one"],
          "options": {
            "project": "packages/my-lib/secondary-one/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/my-lib/secondary-one/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/my-lib/secondary-one/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/my-lib/secondary-one"],
          "options": {
            "jestConfig": "packages/my-lib/secondary-one/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/my-lib/secondary-one/src/**/*.ts",
              "packages/my-lib/secondary-one/src/**/*.html"
            ]
          }
        }
      }
    },
    "my-lib-secondary-two": {
      "projectType": "library",
      "root": "packages/my-lib/secondary-two",
      "sourceRoot": "packages/my-lib/secondary-two/src",
      "prefix": "test-secondary",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/packages/my-lib/secondary-two"],
          "options": {
            "project": "packages/my-lib/secondary-two/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/my-lib/secondary-two/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/my-lib/secondary-two/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/my-lib/secondary-two"],
          "options": {
            "jestConfig": "packages/my-lib/secondary-two/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/my-lib/secondary-two/src/**/*.ts",
              "packages/my-lib/secondary-two/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {}
  },
  "defaultProject": "my-lib"
}
